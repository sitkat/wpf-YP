<Window x:Class="SitnikovaPreliminaryDesign.ContactsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SitnikovaPreliminaryDesign"
        mc:Ignorable="d"
        Title="Контакты" Height="720" Width="1280" WindowStartupLocation="CenterScreen" WindowState="Maximized" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnBack" Style="{StaticResource BackButton}" Click="btnBack_Click"/>
        <Button x:Name="btnCall" Content="Позвонить"
            Width="400"
            Height="50"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            Margin="50 20"
            FontSize="32" Click="btnCall_Click"/>
        <DataGrid Name="dataGrid" Margin="20 100 20 20"/>
    </Grid>
</Window>